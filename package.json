{
  "name": "playnows-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "prettier": "prettier \"./src/**/*.+(js|jsx|css|json|md|mdx)\"",
    "unprettified": "npm run prettier -- --list-different",
    "format": "npm run prettier -- --write",
    "lint": "eslint ./src/ -c ./.eslintrc",
    "fix": "eslint ./src/ --fix",
    "validate": "run-p lint unprettified",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,jsx}": [
      "npm run format",
      "npm run fix",
      "git add"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@n8tb1t/use-scroll-position": "^1.0.43",
    "@reduxjs/toolkit": "^1.3.0-beta.1",
    "@storybook/addon-knobs": "^5.3.17",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "date-fns": "^2.11.0",
    "emoji-mart": "^3.0.0",
    "emotion": "^10.0.27",
    "emotion-theming": "^10.0.27",
    "formik": "^2.1.4",
    "history": "^4.10.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "lodash": "^4.17.15",
    "plyr-react": "^2.2.0",
    "prettier": "^2.0.2",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^16.13.1",
    "react-intersection-observer": "^8.26.1",
    "react-loading-skeleton": "^2.0.1",
    "react-modal": "^3.11.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "react-select": "^3.1.0",
    "react-tooltip": "^4.1.2",
    "react-use": "^13.27.0",
    "redux": "^4.0.5",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "source-map-explorer": "^2.4.1",
    "styled-components": "^5.0.1",
    "use-deep-compare-effect": "^1.3.1",
    "use-onclickoutside": "^0.3.1",
    "uuid": "^7.0.2",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-backgrounds": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-viewport": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/preset-create-react-app": "^2.1.0",
    "@storybook/react": "^5.3.17",
    "@types/jest": "^24.9.1",
    "@types/react": "^16.9.25",
    "babel-plugin-emotion": "^10.0.29",
    "customize-cra": "^0.9.1",
    "customize-cra-react-refresh": "^1.0.1",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "npm-run-all": "^4.1.5",
    "react-app-rewired": "^2.1.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:4000"
}
