{
  "name": "playnows-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "run-p watch:css devserver",
    "devserver": "react-app-rewired start",
    "build": "cross-env NODE_ENV=production run-s build:css build_script",
    "build_script": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "prettier": "prettier \"./src/**/*.+(js|jsx|css|json|md|mdx)\"",
    "unprettified": "npm run prettier -- --list-different",
    "format": "npm run prettier -- --write",
    "lint": "eslint ./src/ -c ./.eslintrc",
    "fix": "eslint ./src/ --fix",
    "validate": "run-p lint unprettified",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build:css": "postcss src/styles/tailwind.pcss -o src/styles/app.css",
    "watch:css": "postcss src/styles/tailwind.pcss -o src/styles/app.css -w",
    "sitemap": "babel-node src/helpers/sitemap-generator.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,jsx}": [
      "npm run format",
      "npm run fix",
      "git add"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "@giphy/js-fetch-api": "^1.8.0",
    "@giphy/react-components": "^1.10.0",
    "@reduxjs/toolkit": "^1.3.5",
    "@sentry/apm": "^5.27.1",
    "@sentry/browser": "^5.27.4",
    "@sentry/react": "^5.27.4",
    "array-move": "^2.2.1",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "emoji-mart": "^3.0.0",
    "formik": "^2.2.5",
    "history": "^4.10.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "rc-slider": "^9.6.2",
    "react": "^16.14.0",
    "react-beautiful-dnd": "^13.0.0",
    "react-collapsed": "^3.0.1",
    "react-dom": "^16.14.0",
    "react-ga": "^3.2.1",
    "react-helmet": "^6.1.0",
    "react-image-file-resizer": "^0.3.8",
    "react-intersection-observer": "^8.30.3",
    "react-linkify": "^1.0.0-alpha",
    "react-localization": "^1.0.15",
    "react-modal": "^3.11.2",
    "react-player": "^2.7.0",
    "react-ripples": "^2.2.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router-sitemap": "^1.2.0",
    "react-scripts": "^3.4.4",
    "react-select": "^3.1.0",
    "react-share": "^4.3.1",
    "react-sortable-hoc": "^1.11.0",
    "react-string-replace": "^0.4.4",
    "react-tooltip": "^4.2.10",
    "react-use": "^13.27.1",
    "react-window": "^1.8.6",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "screenfull": "^5.0.2",
    "use-onclickoutside": "^0.3.1",
    "use-sound": "^1.0.2",
    "uuid": "^7.0.2",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "@hot-loader/react-dom": "^16.14.0",
    "@types/jest": "^24.9.1",
    "@types/react": "^16.9.56",
    "autoprefixer": "^9.8.6",
    "aws-amplify": "^3.3.12",
    "babel-cli": "^6.26.0",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^7.0.2",
    "customize-cra": "^0.9.1",
    "customize-cra-react-refresh": "^1.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.2",
    "postcss-nesting": "^7.0.1",
    "prettier": "^2.1.2",
    "purgecss": "^2.2.1",
    "react-app-rewired": "^2.1.6",
    "react-hot-loader": "^4.13.0",
    "source-map-explorer": "^2.5.0",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "^1.9.0",
    "tailwindcss-border-gradients": "^2.0.3",
    "tailwindcss-children": "^2.0.0",
    "tailwindcss-filters": "^3.0.0",
    "tailwindcss-gradients": "^3.0.0",
    "tailwindcss-truncate-multiline": "^1.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
